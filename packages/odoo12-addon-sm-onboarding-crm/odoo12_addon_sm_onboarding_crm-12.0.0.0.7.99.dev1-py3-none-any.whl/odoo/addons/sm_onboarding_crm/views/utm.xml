<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="utm_source_view_form_inherited" model="ir.ui.view">
        <field name="name">utm.source.form.inherited</field>
        <field name="model">utm.source</field>
        <field name="inherit_id" ref="utm.utm_source_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='name']" position="after">
                <field name="is_onboarding"/>
                <field name="id"/>
            </xpath>
        </field>
    </record>
</odoo>