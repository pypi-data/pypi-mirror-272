{
    "connection": {
        "type": {
            "displayName": "SumoLogic",
            "group": "sumologic",
            "type": "connectorType"
        },
        "region": {
            "type": "text"
        },
        "help": {
            "type": "link",
            "default": "data-sources.html"
        },
        "options": {
            "type": "fields",
            "async_call": {
                "type": "text",
                "hidden": true,
                "optional": true
            },
            "result_limit": {
                "default": 10000,
                "min": 1,
                "max": 10000000,
                "type": "number",
                "previous": "connection.resultSizeLimit"
            },
            "batch_size": {
                "default": 2000,
                "min": 1,
                "max": 10000,
                "type": "number",
                "optional": true,
                "hidden": true
            },
            "time_range": {
                "default": 5,
                "min": 1,
                "max": 10000,
                "type": "number",
                "previous": "connection.timerange",
                "nullable": true
            },
            "timeout": {
                "default": 30,
                "min": 1,
                "max": 3600,
                "hidden": true,
                "type": "number",
                "previous": "connection.timeoutLimit"
            },
            "dialects": {
                "type": "array",
                "hidden": true,
                "optional": true
            },
            "language": {
                "type": "string",
                "default": "stix",
                "optional": true,
                "hidden": true
            },
            "validate_pattern": {
                "type": "boolean",
                "optional": true,
                "hidden": true,
                "previous": "connection.validate_pattern"
            },
            "stix_validator": {
                "type": "boolean",
                "default": false,
                "optional": true,
                "hidden": true,
                "previous": "connection.stix_validator"
            },
            "mapping": {
                "type": "json",
                "optional": true,
                "previous": "connection.mapping",
                "default": {
                    "from_stix_map": {
                        "artifact": {
                            "fields": {
                                "payload_bin": [
                                    "_raw"
                                ]
                            }
                        },
                        "domain-name": {
                            "fields": {
                                "value": [
                                    "_sourcehost"
                                ]
                            }
                        },
                        "x-ibm-finding": {
                            "fields": {
                                "event_count": [
                                    "_messagecount"
                                ],
                                "time_observed": [
                                    "_messagetime"
                                ],
                                "src_device": [
                                    "_collector"
                                ],
                                "start": [
                                    "_receipttime"
                                ]
                            }
                        },
                        "x-oca-event": {
                            "fields": {
                                "code": [
                                    "_messageid"
                                ],
                                "created": [
                                    "_messagetime"
                                ],
                                "agent": [
                                    "_collector"
                                ],
                                "module": [
                                    "_source"
                                ],
                                "provider": [
                                    "_sourcecategory"
                                ],
                                "original_ref.payload_bin": [
                                    "_raw"
                                ],
                                "domain_ref.value": [
                                    "_sourcehost"
                                ],
                                "user_ref.account_login": [
                                    "_useremail"
                                ]
                            }
                        },
                        "x-sumologic-source": {
                            "fields": {
                                "collectorid": [
                                    "_collectorid"
                                ],
                                "sourcename": [
                                    "_sourcename"
                                ]
                            }
                        },
                        "user-account": {
                            "fields": {
                                "user_id": [
                                    "id"
                                ],
                                "account_login": [
                                    "email"
                                ],
                                "display_name": [
                                    "displayName"
                                ],
                                "account_created": [
                                    "createdAt"
                                ],
                                "account_last_login": [
                                    "lastLoginTimestamp"
                                ]
                            }
                        }
                    },
                    "operators": {
                        "ComparisonExpressionOperators.And": "AND",
                        "ComparisonExpressionOperators.Or": "OR",
                        "ComparisonComparators.Equal": "=",
                        "ComparisonComparators.NotEqual": "=",
                        "ComparisonComparators.Like": "=",
                        "ComparisonComparators.In": "OR",
                        "ObservationOperators.Or": "OR",
                        "ObservationOperators.And": "AND"
                    },
                    "to_stix_map": {
                        "_raw": [
                            {
                                "key": "artifact.payload_bin",
                                "transformer": "ToBase64",
                                "object": "artifact"
                            },
                            {
                                "key": "x-oca-event.original_ref",
                                "object": "x-oca-event",
                                "references": "artifact"
                            }
                        ],
                        "_sourcehost": [
                            {
                                "key": "domain-name.value",
                                "object": "domain",
                                "transformer": "ToDomainName"
                            },
                            {
                                "key": "x-oca-event.domain_ref",
                                "object": "x-oca-event",
                                "references": "domain"
                            }
                        ],
                        "_messagecount": [
                            {
                                "key": "x-ibm-finding.event_count",
                                "object": "x-ibm-finding",
                                "transformer": "ToInteger"
                            },
                            {
                                "key": "number_observed",
                                "transformer": "ToInteger"
                            }
                        ],
                        "_messagetime": [
                            {
                                "key": "x-oca-event.created",
                                "object": "x-oca-event",
                                "transformer": "EpochToTimestamp"
                            },
                            {
                                "key": "x-ibm-finding.time_observed",
                                "object": "x-ibm-finding",
                                "transformer": "EpochToTimestamp"
                            },
                            {
                                "key": "first_observed",
                                "transformer": "EpochToTimestamp"
                            },
                            {
                                "key": "last_observed",
                                "transformer": "EpochToTimestamp"
                            }
                        ],
                        "_messageid": {
                            "key": "x-oca-event.code",
                            "object": "x-oca-event"
                        },
                        "_collector": [
                            {
                                "key": "x-oca-event.agent",
                                "object": "x-oca-event"
                            },
                            {
                                "key": "x-ibm-finding.src_device",
                                "object": "x-ibm-finding"
                            }
                        ],
                        "_collectorid": {
                            "key": "x-sumologic-source.collectorid",
                            "object": "x-sumologic-source"
                        },
                        "_receipttime": {
                            "key": "x-ibm-finding.start",
                            "transformer": "EpochToTimestamp",
                            "object": "x-ibm-finding"
                        },
                        "_source": {
                            "key": "x-oca-event.module",
                            "object": "x-oca-event"
                        },
                        "_sourcecategory": {
                            "key": "x-oca-event.provider",
                            "object": "x-oca-event"
                        },
                        "_sourcename": {
                            "key": "x-sumologic-source.sourcename",
                            "object": "x-sumologic-source"
                        },
                        "id": {
                            "key": "user-account.user_id",
                            "object": "user-account"
                        },
                        "email": [
                            {
                                "key": "user-account.account_login",
                                "object": "user-account"
                            },
                            {
                                "key": "x-oca-event.user_ref",
                                "object": "x-oca-event",
                                "references": "user-account"
                            }
                        ],
                        "displayName": {
                            "key": "user-account.display_name",
                            "object": "user-account"
                        },
                        "createdAt": {
                            "key": "user-account.account_created",
                            "object": "user-account"
                        },
                        "lastLoginTimestamp": {
                            "key": "user-account.account_last_login",
                            "object": "user-account"
                        }
                    }
                }
            },
            "unmapped_fallback": {
                "type": "boolean",
                "default": false,
                "optional": true,
                "hidden": true
            },
            "stix_2.1": {
                "type": "boolean",
                "default": false,
                "optional": true,
                "hidden": true
            }
        }
    },
    "configuration": {
        "auth": {
            "type": "fields",
            "access_id": {
                "type": "password"
            },
            "access_key": {
                "type": "password"
            }
        }
    }
}