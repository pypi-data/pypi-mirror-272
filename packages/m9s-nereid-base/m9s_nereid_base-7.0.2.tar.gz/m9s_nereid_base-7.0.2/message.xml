<tryton>
    <data grouped="1">
        <record model="ir.message" id="msg_user_email_unique">
            <field name="text">Email must be unique in a company.</field>
        </record>
        <record model="ir.message" id="invalid_name">
            <field name="text">Invalid folder name:
(1) '.' in folder name (OR)
(2) folder name begins with '/'</field>
        </record>
        <record model="ir.message" id="invalid_file_name">
            <field name="text">Invalid file name:
(1) '..' in file name (OR)
(2) file name contains '/'</field>
        </record>
        <record model="ir.message" id="missing_extension">
            <field name="text">File extension is missing from file name and it is required to guess file type.</field>
        </record>
        <record model="ir.message" id="remove">
            <field name="text">Remove</field>
        </record>
        <record model="ir.message" id="set">
            <field name="text">Set</field>
        </record>
        <record model="ir.message" id="msg_invalid_phone_number">
            <field name="text">The phone number "%(phone)s" is not valid.</field>
        </record>
    </data>
</tryton>
