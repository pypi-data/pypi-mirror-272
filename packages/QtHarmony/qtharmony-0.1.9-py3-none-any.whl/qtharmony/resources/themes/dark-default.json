{
    "Info": {
        "name": "Dark-Default",
        "author": null
    },

    "MainWindow": {
        "object-name": "QMainWindow#main-window",
        
        "": {
            "background-color": "#252525",
            "border": "none",
            "border-radius": "0px"
        }
    },

    "PushButton": {
        "object-name": "QPushButton#button",

        "": {
            "background-color": "#405cf5",
            "border": "none",
            "border-radius": "6px"
        },

        ":hover": {
            "background-color": "#3045B8"
        },

        ":pressed": {
            "background-color": "#283A9A"
        }
    },

    "DropDownMenu": {
        "object-name": "QComboBox#drop-down-menu",

        "": {
            "background-color": "#ffffff",
            "color": "#1A3B5D",
            "border": "1px solid #adb5bd",
            "border-radius": "6px"
        },

        ":hover": {
            "border-color": "#6185DF"
        },

        " QAbstractItemView": {
            "background-color": "#ffffff"
        },

        "::drop-down": {
            "border": "none"
        }
    },

    "Entry": {
        "object-name": "QLineEdit#entry",

        "": {
            "border": "1px solid #adb5bd",
            "background-color": "#ffffff",
            "color": "#1A3B5D",
            "border-radius": "6px"
        },

        ":hover": {
            "border-color": "#6185DF"
        },

        ":focus": {
            "border-color": "#405cf5"
        }
    },

    "DigitalEntry": {
        "object-name": "QSpinBox#digital-entry",

        "": {
            "color": "#1A3B5D",
            "background-color": "#ffffff",
            "border": "1px solid #adb5bd",
            "border-radius": "6px"
        },

        ":hover": {
            "border-color": "#6185DF"
        },

        ":focus": {
            "border-color": "#405cf5"
        }
    }, 

    "RadioButton": {
        "object-name": "QRadioButton#radio-button",

        "": {
            "color": "#ffffff"
        },

        "::indicator": {
            "border": "1px solid #0166fc",
            "border-radius": "7px"
        },

        "::indicator:checked": {
            "background-color": "#0166fc"
        }
    },

    "CheckBox": {
        "object-name": "QCheckBox#check-box",

        "": {
            "color": "#ffffff"
        },

        "::indicator": {
            "border": "1px solid #0166fc",
            "border-radius": "3px"
        },

        "::indicator:checked:enabled": {
            "background-color": "#0166fc"
        },

        "::indicator:checked:disabled": {
            "background-color": "lightgray"
        },

        ":disabled": {
            "color": "gray"
        },

        "::indicator:disabled": {
            "border-color": "gray"
        }
    }, 

    "GroupBox": {
        "object-name": "QGroupBox#group-box",

        "": {
            "background-color": "#323232",
            "border-radius": "6px",
            "border": "none"
        },

        "::title": {
            "color": "#ffffff",
            "background-color": "#404040",
            "border-radius": "6px",
            "subcontrol-position": "top",
            "margin-top": "5px",
            "min-height": "30px",
            "padding-left": "20px",
            "padding-right": "20px"
        }
    },

    "Splitter": {
        "object-name": "QSplitter#splitter"
    }
}